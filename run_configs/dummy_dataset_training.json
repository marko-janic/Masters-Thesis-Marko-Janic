{
    "dataset": "dummy",
    "mode": "train",
    "result_dir_appended_name": "no_overlap_big_dummy",

    "dataset_path": "dataset/dummy_dataset_no_overlap/data",
    "dataset_size": 20000,
    "num_particles": 7,
    "particle_width": 80,
    "particle_height": 80,

    "device": "cuda",
    "epochs": 50,
    "batch_size": 32,
    "learning_rate": 0.001,
    "checkpoint_interval": 1,
    "loss_log_file": "loss_log.txt",

    "train_eval_split": 0.9,
    "latent_dim": 768,
    "num_patch_embeddings": 196,
    "include_class_token": true,
    "only_use_class_token": true,

    "set_cost_class": 1,
    "set_cost_bbox": 5,
    "set_cost_giou": 2,
    "mask_loss_coef": 1,
    "dice_loss_coef": 1,
    "bbox_loss_coef": 5,
    "giou_loss_coef": 2,
    "eos_coef": 0.1
}